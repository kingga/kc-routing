{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    /* "declaration": true, */
    /* "declarationDir": "./dist", */
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
